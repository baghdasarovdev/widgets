{
  "name": "chanel-manager-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "build:docker": "docker compose build",
    "start:dev": "docker-compose -f docker-compose-dev.yml up --remove-orphans --build --watch",
    "start:prod": "docker-compose -f compose.yml up --remove-orphans --build",
    "lint": "next lint && pnpm prettier:check",
    "lint:fix": "pnpm eslint:fix && pnpm prettier:fix",
    "eslint:fix": "next lint --fix",
    "prettier:check": "prettier --check './src/**/*.{ts,tsx,js,jsx}'",
    "prettier:fix": "prettier --write './src/**/*.{ts,tsx,js,jsx}'",
    "prepare": "husky install",
    "build:widget": "parcel build widgets/components/*/*.tsx --no-source-maps --dist-dir ./public/widgets"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@parcel/optimizer-data-url": "^2.13.3",
    "@parcel/transformer-inline-string": "^2.13.3",
    "@reduxjs/toolkit": "^2.2.7",
    "@resido-fhm/ui-kit": "2.0.52-alpha",
    "clsx": "^2.1.1",
    "fullcalendar": "^6.1.15",
    "js-cookie": "^3.0.5",
    "ky": "^1.7.0",
    "next": "14.2.5",
    "next-intl": "^3.17.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.2",
    "react-redux": "^9.1.2",
    "tailwind-merge": "^2.5.5",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/multimonth": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@parcel/config-default": "^2.13.3",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.16.1",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "autoprefixer": "^10.4.20",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-absolute-imports": "^0.0.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "eslint-plugin-unused-imports": "^4.1.3",
    "gh-pages": "^6.3.0",
    "husky": "^8.0.3",
    "next-auth": "^4.24.10",
    "openid-client": "^5.4.0",
    "parcel": "^2.13.3",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "process": "^0.11.10",
    "react-datepicker": "^7.5.0",
    "sass": "^1.77.8",
